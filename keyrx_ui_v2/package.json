{
  "name": "keyrx_ui_v2",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:wasm": "cd ../keyrx_core && wasm-pack build --target web --out-dir ../keyrx_ui_v2/src/wasm/pkg --release -- --features wasm",
    "build": "npm run build:wasm && tsc -b && vite build",
    "build:production": "npm run build:wasm && tsc -b && vite build --mode production",
    "build:analyze": "npm run build && node scripts/analyze-bundle.js",
    "build:production:analyze": "npm run build:production && node scripts/analyze-bundle.js",
    "lint": "eslint .",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "preview": "vite preview",
    "preview:production": "vite preview --mode production",
    "type-check": "tsc -b --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:a11y": "playwright test tests/accessibility.spec.ts",
    "test:lighthouse": "playwright test tests/lighthouse.spec.ts",
    "test:performance": "playwright test tests/performance",
    "test:visual": "playwright test tests/e2e/visual/responsive.spec.ts",
    "test:visual:update": "playwright test tests/e2e/visual/responsive.spec.ts --update-snapshots",
    "test:e2e": "playwright test",
    "test:all": "npm run test && npm run test:a11y && npm run test:lighthouse && npm run test:performance && npm run test:visual"
  },
  "dependencies": {
    "@floating-ui/react": "^0.26.6",
    "@headlessui/react": "^1.7.17",
    "@monaco-editor/react": "^4.7.0",
    "@tanstack/react-query": "^5.17.0",
    "@types/react-window": "^1.8.8",
    "autoprefixer": "^10.4.16",
    "framer-motion": "^11.0.3",
    "lucide-react": "^0.562.0",
    "monaco-editor": "^0.55.1",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "react-use-websocket": "^4.13.0",
    "react-window": "^1.8.10",
    "recharts": "^2.10.3",
    "tailwindcss": "^3.4.0",
    "uuid": "^13.0.0",
    "zustand": "^4.4.5"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@axe-core/react": "^4.8.3",
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.40.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "jsdom": "^27.4.0",
    "msw": "^2.12.7",
    "playwright": "^1.40.1",
    "playwright-lighthouse": "^4.0.0",
    "prettier": "^3.1.1",
    "rollup-plugin-visualizer": "^6.0.5",
    "terser": "^5.44.1",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-top-level-await": "^1.6.0",
    "vite-plugin-wasm": "^3.5.0",
    "vitest": "^1.0.4"
  }
}
